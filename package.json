{
  "name": "http-aws4",
  "version": "0.10.0",
  "description": "Performs AWS Signature Version 4-signed HTTP requests from the command line.",
  "author": "Tim De Pauw <hi@tmdpw.eu> (https://tmdpw.eu/)",
  "engines": {
    "node": ">=4",
    "yarn": "*"
  },
  "license": "MIT",
  "files": [
    "http-aws4.js"
  ],
  "bin": {
    "haws": "http-aws4.js",
    "http-aws4": "http-aws4.js"
  },
  "repository": "timdp/http-aws4",
  "bugs": "https://github.com/timdp/http-aws4/issues",
  "dependencies": {
    "aws-sdk": "^2.32.0",
    "chalk": "^1.1.3",
    "clean-stack": "^1.3.0",
    "emphasize": "^1.2.0",
    "get-stdin": "^5.0.1",
    "lowercase-keys": "^1.0.0",
    "normalize-url": "^1.9.1",
    "pify": "^3.0.0",
    "yargs": "^8.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-env": "^1.2.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier-standard": "^8.0.1",
    "standard": "^10.0.0"
  },
  "scripts": {
    "build": "babel src/http-aws4.js --out-file http-aws4.js",
    "prepublish": "npm run build",
    "test": "standard 'src/**/*.js'",
    "format": "prettier-standard 'src/**/*.js'",
    "precommit": "yarn run lint-staged"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ]
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  }
}
